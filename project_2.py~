#Joseph Davies

class cell:
	new_id = 0
	
	def __init__(self, from_list = [], to_list = []):
		self._id = new_id + 1
		new_id += 1
		
		self._from = from_list
		self._to = to_list
	
	def get_id(self):
		return self._id
	
	@property
	def cells_to(self):
		return self._to
	@cells_to.setter
	def cells_to(self, val):
		self._to = val
	
	@property
	def cells_from(self):
		return self._from
	@cells_from.setter
	def cells_from(self, val):
		self._from = val
